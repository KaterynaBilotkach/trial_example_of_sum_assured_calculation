   "source": [
    "# synthesize data for calculations. df with columns: policy start day; policy end date; subject DOB;  sum assured and subject ID (used as index)\n",
    "\n",
    "# creating empty df\n",
    "trialD=pd.DataFrame()\n",
    "\n",
    "# adding issue_date column with dates for 12 not consecutive month. The date is first day of each month\n",
    "trialD['issue_date']=pd.date_range(start='1/1/2016', periods=12, freq='3MS')\n",
    "\n",
    "# to clreate end_date column, lets add random number of days to issue date (in range 2100 to 5550)\n",
    "td = pd.to_timedelta(np.random.randint(2100,5550, size=len(trialD)), unit='d') # dates to add\n",
    "trialD['end_date']=trialD['issue_date'] + td+pd.offsets.MonthBegin(1) # Rounding to 1st day of month\n",
    "\n",
    "# column DOB, 12 periods, adding 55 days between periods. Rounding DOB to 1st day of month*\n",
    "trialD['DOB']=pd.date_range(start='1/1/1999', periods=12, freq='55D')+pd.offsets.MonthBegin(1)\n",
    "\n",
    " # column with amount of money assured\n",
    "trialD['money']=np.random.randint(300, 10000000, size = 12)\n",
    "\n",
    "# add subject IDs\n",
    "trialD['ID']= range(1, 1+len(trialD))\n",
    "trialD.set_index(trialD.columns[-1], inplace=True) # last columns (ID) becomes the 1st one\n",
    "trialD.reset_index(inplace=True) # setting IDs as index\n",
    "\n",
    "print(\"Synthesized data, 1st 5 rows\")\n",
    "trialD.head(2)"
   ]
  },
