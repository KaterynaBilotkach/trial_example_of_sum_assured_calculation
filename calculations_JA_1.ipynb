   "source": [
    "# using groupby and temporary df in loop, group sums assured by date and age\n",
    "concat_df=pd.DataFrame() # empty dataframe to record results\n",
    "for i in range(m+1,col_n-1):\n",
    "    temp_df=pd.DataFrame({result2.columns[i]: result2.groupby(result2.columns[i])[result2.columns[m]].sum() })\n",
    "    temp_df.index.names=['age']\n",
    "    concat_df = pd.concat([concat_df, temp_df], axis=1)\n",
    "concat_df.head(4)"
   ]
